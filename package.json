{
  "name": "env-docker",
  "version": "1.0.0",
  "keywords": [
    "docker",
    "docker-run",
    "run",
    "env",
    "envc",
    "environments",
    "environment",
    "12factor"
  ],
  "description": "Run a docker command with environment variables populated via .env.",
  "author": "Wil Moore III <wil.moore@wilmoore.com>",
  "contributors": [
    {
      "name": "Wil Moore III",
      "email": "wil.moore@wilmoore.com"
    }
  ],
  "watches": [
    {
      "script": "lint",
      "patterns": [
        "*.js",
        "lib/**/*.js",
        "test/**/*.js"
      ]
    }
  ],
  "scripts": {
    "lint": "jshint bin lib",
    "test": "npm run lint",
    "develop": "rerun-script"
  },
  "files": [
    "*.json",
    "*.js",
    "bin",
    "lib",
    "license",
    "readme.md"
  ],
  "main": "bin/env-docker",
  "bin": {
    "env-docker": "bin/env-docker"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmoore/node-env-docker"
  },
  "bugs": {
    "url": "https://github.com/wilmoore/node-env-docker/issues"
  },
  "devDependencies": {
    "jshint": "^2.5.10",
    "rerun-script": "^0.4.1"
  },
  "dependencies": {
    "commander": "^2.5.1",
    "debug": "^2.1.0",
    "envc": "^2.0.0",
    "shelljs": "^0.3.0"
  },
  "config": {},
  "license": "MIT"
}
